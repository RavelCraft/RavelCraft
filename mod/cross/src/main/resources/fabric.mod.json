{
  "schemaVersion": 1,
  "id": "${project_id}",
  "version": "${project_version}",
  "name": "${project_name}",
  "description": "The mod used for the RavelCraft server.",
  "authors": [
    "Alex"
  ],
  "contact": {
    "homepage": "https://ravelcraft.fr/"
  },

  "license": "CC0-1.0",
  "icon": "assets/ravelcraft/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.connexal.ravelcraft.mod.cross.RavelModDummyInit"
    ],
    "fabric-datagen": [
      "com.connexal.ravelcraft.mod.cross.datagen.RavelModDatagen"
    ]
  },

  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "java": ">=21"
  }
}
